{
  "name": "tip-tracker-turbo",
  "version": "0.0.5",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "generate": "turbo run generate",
