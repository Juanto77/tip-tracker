  "devDependencies": {
    "@tip-tracker-turbo/eslint-config-custom": "workspace:*",
    "@release-it/conventional-changelog": "^5.1.1",
    "release-it": "^15.11.0",
    "turbo": "latest",
    "@antfu/eslint-config": "^2.12.1",
    "eslint": "9.0.0-rc.0"
  },
  "engines": {
    "node": ">=v18.16.0"
  },
  "packageManager": "pnpm@7.32.5"
}